					F.Intrinsic.Control.If(V.Local.sWC,<>,"")
						F.Global.General.SetPassedDataElement("WC",V.Local.sWC)
						F.Intrinsic.String.Build("{0}\PPT_Repl_Upd_Resched_Utility.g2u",V.Caller.GasDir,V.Local.sGabFile)
