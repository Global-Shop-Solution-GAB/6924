Program.Sub.LoadDTDVAll.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
	V.Local.iCnt.Declare
	V.Local.iProgress.Declare
	V.Local.sSQL.Declare
	V.Local.sRet.Declare
	V.Local.sJob.Declare
	V.Local.sSuffix.Declare
	V.Local.fEstHrs.Declare
	V.Local.fRemHrs.Declare
	V.Local.fPcsRem.Declare
	V.Local.sDept.Declare
	V.Local.sCurOper.Declare
	V.Local.dCurOperDate.Declare
	V.Local.sNextOper.Declare
	V.Local.sOutside.Declare
	V.Local.dShipDate.Declare
	V.Local.dDateCheck.Declare(Date,01/01/1900)
	V.Local.bDictExists.Declare
	v.Local.sCondition.Declare
	v.Local.iAPSIndex.Declare
	v.Local.SumESTHRS.Declare

	F.Intrinsic.Control.If(V.DataTable.dtAll.Exists,=,True)
		F.Data.DataTable.Close("dtAll")
		F.Data.DataTable.Close("dtAll2")
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.DataTable.dtJobOperations.Exists,=,True)
		F.Data.DataTable.Close("dtJobOperations")		
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.If(V.DataTable.dtUserFields.Exists,=,True)
		F.Data.DataTable.Close("dtUserFields")		
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.DataTable.dtWoNotes.Exists,=,True)
		F.Data.DataTable.Close("dtWoNotes")		
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.DataTable.dtOrderToWO.Exists,=,True)
		F.Data.DataTable.Close("dtOrderToWO")		
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.DataTable.dtAps.Exists,=,True)
		F.Data.DataTable.Close("dtAps")		
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.DataTable.dtEstHours.Exists,=,True)
		F.Data.DataTable.Close("dtEstHours")		
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.DataTable.dtOutside.Exists,=,True)
		F.Data.DataTable.Close("dtOutside")		
	F.Intrinsic.Control.EndIf
	
	F.Data.Dictionary.Exists("dIndirect",V.Local.bDictExists)
	F.Intrinsic.Control.If(V.Local.bDictExists,=,True)
		F.Data.Dictionary.Close("dIndirect")
	F.Intrinsic.Control.EndIf
	
	V.Global.bLoadData.Set(True)
			
	F.Data.Dictionary.CreateFromSQL("dIndirect","con","SELECT DISTINCT CONCAT(JOB,SUFFIX) AS JS,CONVERT(IF('' = '',1,0),SQL_BIT) FROM IND_LBR_ACCTS;")
	F.Data.Dictionary.SetDefaultReturn("dIndirect",False)

	
	'f.Data.DataTable.CreateFromSQL("dtAll","con","SELECT VJH.PROJECT AS PROJECT, VJH.PHASE AS PHASE, CAST(CASE WHEN FLAG_WO_PRTD = 'Y' OR FLAG_WO_PRTD = 'R' THEN 1 ELSE 0 END AS BIT) AS PRINTED,CAST(CASE WHEN FLAG_WO_RELEASED = 'Y' THEN 1 ELSE 0 END AS BIT) AS RELEASED,CASE WHEN VJH.FLAG_HOLD = 'Y' THEN CONVERT(1,SQL_BIT) WHEN VJH.FLAG_HOLD = 'N' THEN CONVERT(0,SQL_BIT) ELSE CONVERT(0,SQL_BIT) END AS FLAG_HOLD,VJH.JOB,VJH.SUFFIX,PRIORITY,RTRIM(VCM.NAME_CUSTOMER) AS NAME_CUSTOMER,VJH.SALESPERSON,CUSTOMER_PO,COALESCE(VJH.PART,'') AS PART,'' As DISPLAY_PART,VJH.LOCATION,VJH.PRODUCT_LINE,VJH.PART_DESCRIPTION AS DESCRIPTION,DATE_OPENED, CONVERT(NULL,SQL_DATE) AS DATE_START, DATE_DUE,SCHEDULED_DUE_DATE,CONVERT(0.00,SQL_FLOAT) AS EST_HRS,NUM_HOURS AS ACT_HRS,CONVERT(0.00,SQL_FLOAT) AS REM_HRS,'' AS DEPT,CONVERT(NULL,SQL_DATE) AS CUR_OPER_DATE,'' AS CUR_OPER,'' AS NEXT_OPER,'' AS OUTSIDE,CONVERT(NULL,SQL_DATE) AS SHIP_TO_OP,QTY_ORDER,QTY_ORDER - QTY_COMPLETED AS PCS_REM1, '' AS WO_NOTE, '' AS WO_NOTE2, CONCAT(VJH.JOB,VJH.SUFFIX) AS JS,CONVERT(0,SQL_BIT) AS IND_FLAG,CONVERT(0,SQL_BIT) AS LATE,'' AS PCS_REM FROM V_JOB_HEADER VJH LEFT JOIN V_CUSTOMER_MASTER VCM ON VJH.CUSTOMER=VCM.CUSTOMER WHERE DATE_CLOSED = '1900-1-1' ORDER BY DATE_DUE;", True)
	
	'Added User_5 from V_Order_Header
	f.Data.DataTable.CreateFromSQL("dtAll","con","SELECT VJH.PROJECT AS PROJECT, VJH.PHASE AS PHASE, CAST(CASE WHEN FLAG_WO_PRTD = 'Y' OR FLAG_WO_PRTD = 'R' THEN 1 ELSE 0 END AS BIT) AS PRINTED,CAST(CASE WHEN FLAG_WO_RELEASED = 'Y' THEN 1 ELSE 0 END AS BIT) AS RELEASED,CASE WHEN VJH.FLAG_HOLD = 'Y' THEN CONVERT(1,SQL_BIT) WHEN VJH.FLAG_HOLD = 'N' THEN CONVERT(0,SQL_BIT) ELSE CONVERT(0,SQL_BIT) END AS FLAG_HOLD,VJH.JOB,VJH.SUFFIX,PRIORITY,RTRIM(VCM.NAME_CUSTOMER) AS NAME_CUSTOMER,VJH.SALESPERSON,VJH.CUSTOMER_PO,COALESCE(VJH.PART,'') AS PART,'' As DISPLAY_PART,VJH.LOCATION,VJH.PRODUCT_LINE,VJH.PART_DESCRIPTION AS DESCRIPTION,DATE_OPENED, CONVERT(NULL,SQL_DATE) AS DATE_START, VJH.DATE_DUE,SCHEDULED_DUE_DATE,CONVERT(0.00,SQL_FLOAT) AS EST_HRS,NUM_HOURS AS ACT_HRS,CONVERT(0.00,SQL_FLOAT) AS REM_HRS,'' AS DEPT,CONVERT(NULL,SQL_DATE) AS CUR_OPER_DATE,'' AS CUR_OPER,'' AS NEXT_OPER,'' AS OUTSIDE,CONVERT(NULL,SQL_DATE) AS SHIP_TO_OP,QTY_ORDER,QTY_ORDER - QTY_COMPLETED AS PCS_REM1, '' AS WO_NOTE, '' AS WO_NOTE2, CONCAT(VJH.JOB,VJH.SUFFIX) AS JS,CONVERT(0,SQL_BIT) AS IND_FLAG,CONVERT(0,SQL_BIT) AS LATE,'' AS PCS_REM,VOH.USER_5 FROM V_JOB_HEADER VJH LEFT JOIN V_CUSTOMER_MASTER VCM ON VJH.CUSTOMER=VCM.CUSTOMER LEFT JOIN V_ORDER_LINES VOH ON VJH.SALES_ORDER=VOH.ORDER_NO and VJH.SALES_ORDER_LINE = SUBSTRING(VOH.RECORD_NO,1,3) WHERE DATE_CLOSED = '1900-1-1' ORDER BY VJH.DATE_DUE;", True)
		
	f.Data.DataTable.CreateFromSQL("dtJobOperations", "con", "SELECT JOB, SUFFIX, DATE_START FROM V_JOB_OPERATIONS WHERE LMO='L' and SEQ<'990000' ORDER BY JOB, SUFFIX, SEQ", True)
	
	f.Data.DataTable.CreateFromSQL("dtUserFields","con","SELECT JOB, SUFFIX, USER_6 FROM WO_USER_FLDS ORDER BY JOB, SUFFIX",True)
	
	f.Data.DataTable.CreateFromSQL("dtWoNotes","con","SELECT JOB, SUFFIX, NOTES FROM GAB_4169_WO_NOTES ORDER BY JOB, SUFFIX",True)
	
	f.Data.DataTable.CreateFromSQL("dtOrderToWO","con","SELECT VJH.JOB, VJH.SUFFIX, OWO.ORDER_NO, VOB.NAME_CUSTOMER FROM JOB_HEADER VJH LEFT OUTER JOIN ORDER_TO_WO OWO ON VJH.JOB = OWO.JOB AND VJH.SUFFIX = OWO.SUFFIX LEFT OUTER JOIN V_ORDER_BILL_TO VOB ON OWO.ORDER_NO = VOB.ORDER_NO WHERE NAME_CUSTOMER<>'' ORDER BY VJH.JOB, VJH.SUFFIX",True)
			
	f.Data.DataTable.CreateFromSQL("dtEstHours","con","SELECT A.JSS, A.JOB AS JOB, A.SFX AS SFX, A.SEQ, A.SEQTYPE, A.SEQRES, A.PCSREMAIN, A.STARTDATE, A.OPCODE,A.HOURSORIG, A.INITSU, IF(WORK_GROUP <> '',D.WC_DEPT,B.WC_DEPT) AS DEPT FROM APSV3_JBLINES A LEFT JOIN WORKCENTERS B ON A.SEQRES = B.MACHINE LEFT JOIN WORKGROUP_HEAD C ON A.SEQRES = C.WORK_GROUP LEFT JOIN WORKCENTERS D ON C.PROTOTYPE_WC = D.MACHINE WHERE A.SEQTYPE = 1 ORDER BY A.JSS;",True)	
	
	f.Data.DataTable.CreateFromSQL("dtOutside","con","SELECT A.JSS, A.JOB AS JOB, A.SFX AS SFX, A.SEQ, A.SEQTYPE, A.SEQRES, A.PCSREMAIN, A.STARTDATE, A.OPCODE,A.HOURSORIG, A.INITSU, IF(WORK_GROUP <> '',D.WC_DEPT,B.WC_DEPT) AS DEPT FROM APSV3_JBLINES A LEFT JOIN WORKCENTERS B ON A.SEQRES = B.MACHINE LEFT JOIN WORKGROUP_HEAD C ON A.SEQRES = C.WORK_GROUP LEFT JOIN WORKCENTERS D ON C.PROTOTYPE_WC = D.MACHINE WHERE A.COMPLDATE IS NULL ORDER BY A.JSS;",True)	
		
	f.Data.DataTable.CreateFromSQL("dtAps","con","SELECT A.JOB,A.SFX,A.SEQ,A.SEQTYPE,A.SEQRES,A.PCSREMAIN,A.STARTDATE,IF(WORK_GROUP <> '',D.WC_DEPT,B.WC_DEPT) AS DEPT, A.OPCODE AS OPCODE FROM APSV3_JBLINES A LEFT JOIN WORKCENTERS B ON A.SEQRES = B.MACHINE LEFT JOIN WORKGROUP_HEAD C ON A.SEQRES = C.WORK_GROUP LEFT JOIN WORKCENTERS D ON C.PROTOTYPE_WC = D.MACHINE WHERE A.COMPLDATE IS NULL AND A.SEQTYPE <> '2' ORDER BY A.JOB,A.SFX,A.SEQ;",True)	
		
	'Long Part
	f.Intrinsic.Control.If(v.Global.bLongPart)
		F.Data.DataTable.AddDisplayPartColumn("dtAll",0,"PART","DISPLAY_PART")
	f.Intrinsic.Control.Else
		F.Data.DataTable.CopyColumn("dtAll","PART","DISPLAY_PART")
	f.Intrinsic.Control.EndIf
		
	'Multi-Part
	F.Data.DataTable.AddColumn("dtAll","Multi",Boolean,False)
	F.Data.Dictionary.CreateFromSQL("dMulti","con","Select Concat(Job,Suffix),Cast(1 As Bit) From Job_Mult_Parts;")
	F.Data.Dictionary.SetDefaultReturn("dMulti",False)
	F.Data.DataTable.FillFromDictionary("dtAll","dMulti","JS","Multi")
	F.Data.Dictionary.Close("dMulti")	
	
	F.Intrinsic.Control.If(V.DataTable.dtAll.RowCount, =, 0)
		Gui.FrmMaster..ShowAlert("myAlert", "No Records Found", "No Work Orders Found.", V.Enum.Image!WARNING_BLACK)
	
	F.Intrinsic.Control.Else
	Gui.FrmMaster.lblStatus.Visible(True)
	Gui.FrmMaster.pbStatus.Visible(True)
	Gui.FrmMaster.lblStatus.ForeColor(V.Color.Black)
	Gui.FrmMaster.lblStatus.FontSize(8)
	Gui.FrmMaster.lblStatus.Caption("Loading Work Orders....")
	Gui.FrmMaster.pbStatus.Min(0)
	Gui.FrmMaster.pbStatus.Max(V.DataTable.dtAll.RowCount)

	F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtAll.RowCount--,1)
		F.Intrinsic.Math.Add(V.Local.iProgress,1,V.Local.iProgress)
		Gui.FrmMaster.pbStatus.Value(V.Local.iProgress)
		V.Local.sJob.Set(V.DataTable.dtAll(V.Local.iCnt).JOB!FieldVal)
		V.Local.sSuffix.Set(V.DataTable.dtAll(V.Local.iCnt).SUFFIX!FieldVal)	
		V.Local.sDept.Set("")
		V.Local.sCurOper.Set("")
		V.Local.dCurOperDate.Set(01/01/1900)
		V.Local.sNextOper.Set("")
		V.Local.sOutside.Set("")
		V.Local.dShipDate.Set(01/01/1900)
		v.Local.fEstHrs.Set(0)		
		V.Local.fRemHrs.Set(0)
		v.Local.SumESTHRS.Set(0)

		'Getting value for DATE_START from job_Operations
		f.Intrinsic.String.Build("JOB = '{0}' AND SUFFIX = '{1}'",V.Local.sJob,V.Local.sSuffix,v.Local.sCondition)
		f.Data.DataTable.Select("dtJobOperations",v.Local.sCondition,v.Local.sRet)
		
		f.Intrinsic.Control.If(v.Local.sRet,<>,"***NORETURN***")
			f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"DATE_START",v.DataTable.dtJobOperations(v.Local.sRet(0)).DATE_START!FieldVal)
		f.Intrinsic.Control.EndIf
		
		'Getting value for NOTES from GAB_4169_WO_NOTES
		f.Intrinsic.String.Build("JOB = '{0}' AND SUFFIX = '{1}'",V.Local.sJob,V.Local.sSuffix,v.Local.sCondition)
		f.Data.DataTable.Select("dtWONotes",v.Local.sCondition,v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet,<>,"***NORETURN***")
			f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"WO_NOTE",v.DataTable.dtWONotes(v.Local.sRet(0)).NOTES!FieldValTrim)
		f.Intrinsic.Control.EndIf
		
		'Getting value for NOTES from WO_USER_FLDS
		f.Intrinsic.String.Build("JOB = '{0}' AND SUFFIX = '{1}'",V.Local.sJob,V.Local.sSuffix,v.Local.sCondition)
		f.Data.DataTable.Select("dtUserFields",v.Local.sCondition,v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet,<>,"***NORETURN***")		
			f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"WO_NOTE2",v.DataTable.dtUserFields(v.Local.sRet(0)).USER_6!FieldValTrim)
		f.Intrinsic.Control.EndIf
		
		'Getting value for NAME_CUSTOMER from ORDER_TO_WO
		f.Intrinsic.String.Build("JOB = '{0}' AND SUFFIX = '{1}'",V.Local.sJob,V.Local.sSuffix,v.Local.sCondition)
		f.Data.DataTable.Select("dtOrderToWO",v.Local.sCondition,v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet,<>,"***NORETURN***")
			f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"NAME_CUSTOMER",v.DataTable.dtOrderToWO(v.Local.sRet(0)).NAME_CUSTOMER!FieldValTrim)
		f.Intrinsic.Control.EndIf
		
		
		f.Intrinsic.String.Build("JOB = '{0}' AND SFX = '{1}'",V.Local.sJob,V.Local.sSuffix,v.Local.sCondition)
		f.Data.DataTable.Select("dtAps",v.Local.sCondition,v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet,=,"***NORETURN***")
			V.Local.sCurOper.Set("WIPTOFG")
			V.Local.fPcsRem.Set(0)
			V.Local.sNextOper.Set("")
			V.Local.sDept.Set("")
		f.Intrinsic.Control.else
			f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
			f.Intrinsic.Control.for(v.Local.iAPSIndex,0,v.Local.sRet.UBound)
				F.Intrinsic.Control.SelectCase(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).SEQTYPE!FieldVal)
					'LABOR
					F.Intrinsic.Control.Case(1)
						F.Intrinsic.Control.If(V.Local.sCurOper,=,"")
							V.Local.sDept.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).DEPT!FieldValTrim)
							V.Local.sCurOper.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).SEQRES!FieldValTrim)
								F.Intrinsic.Control.If(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).STARTDATE!FieldVal,=,"")
									V.Local.dCurOperDate.Set(01/01/1900)
								F.Intrinsic.Control.Else
									V.Local.dCurOperDate.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).STARTDATE!FieldVal)
								F.Intrinsic.Control.EndIf
							V.Local.fPcsRem.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).PCSREMAIN!FieldValFloat)
						F.Intrinsic.Control.Else
							V.Local.sNextOper.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).SEQRES!FieldValTrim)
							V.Local.fPcsRem.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).SEQRES!FieldValTrim)
							F.Intrinsic.Control.ExitFor(v.Local.iAPSIndex)
						F.Intrinsic.Control.EndIf	
					'OUTSIDE
					F.Intrinsic.Control.Case(3)
						F.Intrinsic.Control.If(V.Local.sCurOper,=,"")
							V.Local.sDept.Set("")
							V.Local.sCurOper.Set("OUTSIDE")
							F.Intrinsic.Control.If(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).STARTDATE!FieldVal,=,"")
								V.Local.dCurOperDate.Set(01/01/1900)
							F.Intrinsic.Control.Else
								V.Local.dCurOperDate.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).STARTDATE!FieldVal)
							F.Intrinsic.Control.EndIf
							V.Local.fPcsRem.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).PCSREMAIN.FieldValFloat)
						F.Intrinsic.Control.Else
							F.Intrinsic.Control.If(V.Local.sOutside,=,"")
								V.Local.sNextOper.Set("OUTSIDE")
							F.Intrinsic.Control.Else
								V.Local.sNextOper.Set("OUTSIDE")
							F.Intrinsic.Control.EndIf
							F.Intrinsic.Control.ExitFor(v.Local.iAPSIndex)
						F.Intrinsic.Control.EndIf
					'TASK
					F.Intrinsic.Control.Case(6)
						F.Intrinsic.Control.If(V.Local.sCurOper,=,"")
							V.Local.sDept.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).DEPT!FieldValTrim)
							'BUILD CURRENT OP...TASK(OPCODE)
							F.Intrinsic.String.Build("{0} ({1})",v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).SEQRES!FieldValTrim,v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).OPCODE!FieldValTrim,V.Local.sCurOper)
								F.Intrinsic.Control.If(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).STARTDATE!FieldVal,=,"")
									V.Local.dCurOperDate.Set(01/01/1900)
								F.Intrinsic.Control.Else
									V.Local.dCurOperDate.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).STARTDATE!FieldVal)
								F.Intrinsic.Control.EndIf
							V.Local.fPcsRem.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).PCSREMAIN!FieldValFloat)
						F.Intrinsic.Control.Else
							'BUILD CURRENT OP...TASK(OPCODE)
							F.Intrinsic.String.Build("{0} ({1})",v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).SEQRES!FieldValTrim,v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).OPCODE!FieldValTrim,V.Local.sNextOper)
							V.Local.fPcsRem.Set(v.DataTable.dtAps(v.Local.sRet(v.Local.iAPSIndex)).PCSREMAIN!FieldValFloat)
							f.Intrinsic.Control.ExitFor(v.Local.iAPSIndex)
						F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.EndSelect
			f.Intrinsic.Control.Next(v.Local.iAPSIndex)			
		f.Intrinsic.Control.EndIf
		
		f.Intrinsic.String.Build("SEQTYPE = '3' AND JOB = '{0}' AND SFX = '{1}'",V.Local.sJob,V.Local.sSuffix,v.Local.sCondition)
		f.Data.DataTable.Select("dtOutside",v.Local.sCondition,v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet,<>,"***NORETURN***")
			F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
			f.Intrinsic.String.Build("{0}*!*{1}",v.DataTable.dtOutside(v.Local.sRet(0)).SEQRES!FieldValTrim,v.DataTable.dtOutside(v.Local.sRet(0)).STARTDATE!FieldVal,v.Local.sOutside)
			F.Intrinsic.String.Split(V.Local.sOutside,"*!*",V.Local.sOutside)
			F.Intrinsic.Control.If(V.Local.sOutside(1),=,"OOB",OR,V.Local.sOutside(1),=,"")
				V.Local.dShipDate.Set(01/01/1900)
			F.Intrinsic.Control.Else
				V.Local.dShipDate.Set(V.Local.sOutside(1))
			F.Intrinsic.Control.EndIf
			V.Local.sOutside.Set(V.Local.sOutside(0))
		f.Intrinsic.Control.EndIf
				
		f.Intrinsic.String.Build("SEQTYPE = '1' AND JOB = '{0}' AND SFX = '{1}'",V.Local.sJob,V.Local.sSuffix,v.Local.sCondition)
		f.Data.DataTable.Select("dtEstHours",v.Local.sCondition,v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet,<>,"***NORETURN***")
			F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
			f.Intrinsic.Control.for(v.Local.iAPSIndex,0,v.Local.sRet.UBound)
				v.Local.fEstHrs.Set(0)
				f.Intrinsic.Math.Add(v.DataTable.dtEstHours(v.Local.sRet(v.Local.iAPSIndex)).HOURSORIG!FieldValFloat,v.DataTable.dtEstHours(v.Local.sRet(v.Local.iAPSIndex)).INITSU!FieldValFloat,v.Local.fEstHrs)				
				f.Intrinsic.Math.Add(v.local.SumESTHRS,v.Local.fEstHrs,v.Local.SumESTHRS)
			f.Intrinsic.Control.Next(v.Local.iAPSIndex)
			
			F.Intrinsic.Control.If(V.Local.SumESTHRS,=,0)
				V.Local.SumESTHRS.Set(0.00)
				V.Local.fRemHrs.Set(0.00)
			F.Intrinsic.Control.Else
				F.Intrinsic.Math.Sub(V.Local.SumESTHRS,V.DataTable.dtAll(V.Local.iCnt).ACT_HRS!FieldValFloat,V.Local.fRemHrs)
			F.Intrinsic.Control.EndIf
		f.Intrinsic.Control.EndIf
		
		F.Intrinsic.Control.If(V.Local.dCurOperDate,=,V.Local.dDateCheck)
		F.Intrinsic.Control.AndIf(V.Local.dShipDate,=,V.Local.dDateCheck)			
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"EST_HRS",V.Local.SumESTHRS,"REM_HRS",V.Local.fRemHrs,"DEPT",V.Local.sDept,"CUR_OPER",V.Local.sCurOper,"NEXT_OPER",V.Local.sNextOper,"OUTSIDE",V.Local.sOutside,"PCS_REM",V.Local.fPcsRem)		
		F.Intrinsic.Control.ElseIf(V.Local.dCurOperDate,=,V.Local.dDateCheck)			
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"EST_HRS",V.Local.SumESTHRS,"REM_HRS",V.Local.fRemHrs,"DEPT",V.Local.sDept,"CUR_OPER",V.Local.sCurOper,"NEXT_OPER",V.Local.sNextOper,"OUTSIDE",V.Local.sOutside,"SHIP_TO_OP",V.Local.dShipDate,"PCS_REM",V.Local.fPcsRem)
		F.Intrinsic.Control.ElseIf(V.Local.dShipDate,=,V.Local.dDateCheck)			
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"EST_HRS",V.Local.SumESTHRS,"REM_HRS",V.Local.fRemHrs,"DEPT",V.Local.sDept,"CUR_OPER",V.Local.sCurOper,"CUR_OPER_DATE",V.Local.dCurOperDate,"NEXT_OPER",V.Local.sNextOper,"OUTSIDE",V.Local.sOutside,"PCS_REM",V.Local.fPcsRem)
		F.Intrinsic.Control.Else			
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"EST_HRS",V.Local.SumESTHRS,"REM_HRS",V.Local.fRemHrs,"DEPT",V.Local.sDept,"CUR_OPER",V.Local.sCurOper,"CUR_OPER_DATE",V.Local.dCurOperDate,"NEXT_OPER",V.Local.sNextOper,"OUTSIDE",V.Local.sOutside,"SHIP_TO_OP",V.Local.dShipDate,"PCS_REM",V.Local.fPcsRem)
		F.Intrinsic.Control.EndIf

		F.Intrinsic.Control.If(V.DataTable.dtAll(V.Local.iCnt).Multi!FieldVal)
			F.Data.DataTable.SetValue("dtAll",V.Local.iCnt,"PART","MULTI-PART","DISPLAY_PART","MULTI-PART")
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Next(V.Local.iCnt)
	
	F.Intrinsic.Control.EndIf
